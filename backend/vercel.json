{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.ts", "src/**/*.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/api/$1.ts"
      }
    ]
  }
  